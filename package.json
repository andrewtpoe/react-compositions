{
  "name": "react-compositions",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://andrewtpoe.github.io/react-compositions",
  "dependencies": {
    "gh-pages": "^1.0.0",
    "normalize.css": "^7.0.0",
    "polished": "^1.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "reactive-container": "git+ssh://git@github.decisiv.net/Development/reactive-container.git#28ca4d22fdec2e05aaee20329cf60bafb5b023ab",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-markdown": "^2.5.0",
    "react-router-dom": "^4.1.2",
    "react-split-pane": "^0.1.65",
    "styled-components": "^2.1.1",
    "styled-components-modifiers": "git+ssh://git@github.decisiv.net/Development/styled-components-modifiers.git#7a53390edbcee6981a6cf1b52cf8ab8c08e09dd3",
    "styled-components-theme-validator": "git+ssh://git@github.decisiv.net/Development/styled-components-theme-validator.git#0b598c3d8cb1cfb5d9101e9d32217bd830686cf5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-react-app": "^3.0.0",
    "cross-env": "^5.0.0",
    "react-app-rewire-styled-components": "^2.0.0",
    "react-app-rewired": "^1.0.11",
    "react-hot-loader": "next",
    "react-intl-translations-manager": "^5.0.0",
    "react-scripts": "^1.0.10",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "build": "npm run translate && cross-env NODE_PATH=src/ react-app-rewired build",
    "build:clean": "rimraf ./build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "start": "cross-env NODE_PATH=src react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom",
    "translate": "npm run translations:clean && npm run translations:extract && npm run translations:generate",
    "translations:clean": "rimraf ./src/translations/extracted-messages && rimraf ./src/translations/en.json && echo \"{}\" > ./src/translations/en.json",
    "translations:extract": "cross-env NODE_ENV=production babel ./src >/dev/null",
    "translations:generate": "cross-env NODE_ENV=production babel-node scripts/translationRunner.js"
  }
}
